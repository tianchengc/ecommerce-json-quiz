{
  "name": "ecommerce-json-quiz",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Tiancheng Chen",
  "description": "A lightweight, embeddable tea quiz widget built with Next.js and TailwindCSS",
  "keywords": [
    "nextjs",
    "quiz",
    "tailwindcss",
    "tea",
    "recommendation",
    "widget",
    "cloudflare"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "@google/genai": "^1.41.0",
    "next": "^15.5.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "fs-extra": "^11.3.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "wrangler": "^3.78.0"
  }
}
